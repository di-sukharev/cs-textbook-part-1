/**
 * 4-way demultiplexor:
 * {a, b, c, d} = {in, 0, 0, 0} if sel == 00
 *                {0, in, 0, 0} if sel == 01
 *                {0, 0, in, 0} if sel == 10
 *                {0, 0, 0, in} if sel == 11
 */

CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
        // –ó–¥–µ—Å—å –Ω—É–∂–Ω–æ –Ω–∞–ø–∏—Å–∞—Ç—å HDL –∫–æ–¥, –∫–æ—Ç–æ—Ä—ã–π —Ä–µ–∞–ª–∏–∑—É–µ—Ç —Ç–∞–±–ª–∏—Ü—É –∏—Å—Ç–∏–Ω–Ω–æ—Å—Ç–∏ —ç—Ç–æ–≥–æ —á–∏–ø–∞ üåû
        // –í—ã –º–æ–∂–µ—Ç–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –≤—Å–µ —á–∏–ø—ã, –∫–æ—Ç–æ—Ä—ã–µ —É–∂–µ —Å–æ–±—Ä–∞–ª–∏
}